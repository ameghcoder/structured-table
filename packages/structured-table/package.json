        "dev": "tsup --watch",
        "clean": "rimraf dist",
        "prepublishOnly": "npm run build"
    },
    "peerDependencies": {
        "react": ">=18",
        "react-dom": ">=18"
    },
    "devDependencies": {
        "@types/node": "^25.0.2",
        "@types/react": "^19.2.7",
        "rimraf": "^6.1.2",
        "tsup": "^8.5.1",
        "typescript": "^5.4.0"
    },
    "dependencies": {
        "nanoid": "^5.1.6"
    }
