{
    "name": "structured-table",
    "version": "0.1.0",
    "description": "Structured, themeable, SSR-friendly tables for React and Sanity.",
    "licenses": [
        {
            "type": "MIT",
            "url": "https://github.com/ameghcoder/structured-table/blob/main/LICENSE"
        }
    ],
    "author": {
        "name": "Yashraj Yadav",
        "url": "https://github.com/ameghcoder"
    },
    "keywords": [
        "react",
        "table",
        "structured-data",
        "ssr",
        "ui-component",
        "sanity tables"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/ameghcoder/structured-table",
        "directory": "packages/structured-table"
    },
    "bugs": {
        "url": "https://github.com/ameghcoder/structured-table/issues"
    },
